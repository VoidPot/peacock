{
  "name": "peacock-d3f3",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "migrate": "prisma migrate dev",
    "backup": "ts-node ./prisma/backup.ts",
    "seed": "prisma db seed",
    "seed:copy": "prisma db seed copy",
    "setup": "prisma generate && prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.3.2",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^5.4.2",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/express": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@remix-run/server-runtime": "^2.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "daisyui": "^3.9.3",
    "express": "^4.18.2",
    "flowbite": "^1.8.1",
    "flowbite-react": "^0.6.4",
    "fs-extra": "^11.1.1",
    "html2canvas": "^1.4.1",
    "isbot": "^3.7.0",
    "jspdf": "^2.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "prom-client": "^15.0.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-toastify": "^9.1.3",
    "remix-hook-form": "^2.0.3",
    "sharp": "^0.32.6",
    "tiny-invariant": "^1.3.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.7.4",
    "@types/eslint": "^8.44.6",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^11.0.3",
    "@types/lodash": "^4.14.200",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/react-datepicker": "^4.19.1",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "happy-dom": "^12.9.1",
    "msw": "^1.3.2",
    "npm-run-all": "^4.1.5",
